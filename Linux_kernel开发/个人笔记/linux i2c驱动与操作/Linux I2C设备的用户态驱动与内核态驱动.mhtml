#include &lt;linux/i2c-dev.h&gt;

#define ADXL345_DEVICE_ADDR     0x53    // I2C ADDRESS, assumes ALT address=
 pin low
#define ADXL345_DEVICE_ID       0xE5    // Device ID

//=E5=AF=84=E5=AD=98=E5=99=A8=E5=9C=B0=E5=9D=80
#define ADXL345_REG_DEVID       0x00    // Device ID
#define ADXL345_REG_BW_RATE     0x2C    // Data rate and power mode control
#define ADXL345_REG_POWER_CTL   0x2D    // Power-saving features control
#define ADXL345_REG_DATA_FORMAT 0x31    // Data format control
#define ADXL345_REG_DATAX0      0x32    // X-axis data 0
#define ADXL345_REG_DATAX1      0x33    // X-axis data 1
#define ADXL345_REG_DATAY0      0x34    // Y-axis data 0
#define ADXL345_REG_DATAY1      0x35    // Y-axis data 1
#define ADXL345_REG_DATAZ0      0x36    // Z-axis data 0
#define ADXL345_REG_DATAZ1      0x37    // Z-axis data 1

ADXL345::ADXL345()
{
    fd=3D-1;
}

ADXL345::~ADXL345()
{
    if(fd&gt;=3D0)
        close(fd);
}

ADXL345_error_t ADXL345::begin(const char* i2cDev)
{
    //=E6=89=93=E5=BC=80I2C=E9=80=82=E9=85=8D=E5=99=A8
    fd=3Dopen(i2cDev,O_RDWR);
    if(fd&lt;0)
        return ADXL345_NO_I2C_ADAPTER;
    //=E7=A6=81=E7=94=A810=E4=BD=8D=E5=9C=B0=E5=9D=80
    ioctl(fd,I2C_TENBIT,0);
    //=E8=AE=BE=E7=BD=AEI2C=E4=BB=8E=E8=AE=BE=E5=A4=87=E5=9C=B0=E5=9D=80
    ioctl(fd,I2C_SLAVE,ADXL345_DEVICE_ADDR);
    //=E8=AF=BB=E5=8F=96Device ID
    uint8_t devID=3Di2cReadU8(ADXL345_REG_DEVID);
    //=E5=A6=82=E6=9E=9C=E4=B8=8D=E6=98=AFADXL345
    if(devID!=3DADXL345_DEVICE_ID)
        return ADXL345_NO_DEVICE;
    //=E5=90=AF=E5=8A=A8=E7=94=B5=E6=BA=90=EF=BC=8C=E5=BC=80=E5=A7=8B=E6=B5=
=8B=E9=87=8F
    i2cWriteU8(ADXL345_REG_POWER_CTL,0x08);
    return ADXL345_OK;
}

ADXL345_range_t ADXL345::getRange()
{
    uint8_t format=3Di2cReadU8(ADXL345_REG_DATA_FORMAT);
    return (ADXL345_range_t)(format&amp;0x03);
}

void ADXL345::setRange(ADXL345_range_t range)
{
    uint8_t format=3Di2cReadU8(ADXL345_REG_DATA_FORMAT);
    format&amp;=3D~0x0F;
    format|=3Drange;
    //=E7=A1=AE=E4=BF=9DFULL-RES=E7=BD=AE=E4=BD=8D
    format|=3D0x08;
    i2cWriteU8(ADXL345_REG_DATA_FORMAT,format);
}

ADXL345_dataRate_t ADXL345::getDataRate()
{
    uint8_t rate=3Di2cReadU8(ADXL345_REG_BW_RATE);
    return (ADXL345_dataRate_t)(rate&amp;0x0F);
}

void ADXL345::setDataRate(ADXL345_dataRate_t dataRate)
{
    i2cWriteU8(ADXL345_REG_BW_RATE,dataRate);
}

double ADXL345::getX()
{
    int16_t rawX=3Di2cReadS16(ADXL345_REG_DATAX0);
    return rawX*(9.8*0.004);
}

double ADXL345::getY()
{
    int16_t rawY=3Di2cReadS16(ADXL345_REG_DATAY0);
    return rawY*(9.8*0.004);
}

double ADXL345::getZ()
{
    int16_t rawZ=3Di2cReadS16(ADXL345_REG_DATAZ0);
    return rawZ*(9.8*0.004);
}

void ADXL345::i2cWriteU8(uint8_t addr,uint8_t data)
{
    if(fd&lt;0)
        return;
    uint8_t buf[2];
    buf[0]=3Daddr;
    buf[1]=3Ddata;
    write(fd,buf,2);
}

uint8_t ADXL345::i2cReadU8(uint8_t addr)
{
    if(fd&lt;0)
        return 0;
    write(fd,&amp;addr,1);
    uint8_t data=3D0;
    read(fd,&amp;data,1);
    return data;
}

int16_t ADXL345::i2cReadS16(uint8_t addr)
{
    if(fd&lt;0)
        return 0;
    write(fd,&amp;addr,1);
    uint8_t buf[2];
    read(fd,buf,2);
    return buf[0]|(buf[1]&lt;&lt;8);
}

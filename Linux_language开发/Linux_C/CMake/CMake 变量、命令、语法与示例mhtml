IF (WIN32)
<span class=3D"pl-en">ELSE</span> ()
<span class=3D"pl-en">ENDIF</span> ()
=E9=85=8D=E5=90=88ELSEIF=E4=BD=BF=E7=94=A8,=E5=8F=AF=E8=83=BD=E7=9A=84=E5=
=86=99=E6=B3=95=E6=98=AF=E8=BF=99=E6=A0=B7:

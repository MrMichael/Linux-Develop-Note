pan>(<span class=3D"pl-s1">self</span>, <span class=3D"pl-s1">error</span>)=
:
        <span class=3D"pl-en">super</span>().<span class=3D"pl-en">connect_=
failed</span>(<span class=3D"pl-s1">error</span>)
        <span class=3D"pl-en">print</span>(<span class=3D"pl-s">"[%s] Conne=
ction failed: %s"</span> <span class=3D"pl-c1">%</span> (<span class=3D"pl-=
s1">self</span>.<span class=3D"pl-s1">mac_address</span>, <span class=3D"pl=
-en">str</span>(<span class=3D"pl-s1">error</span>)))

    <span class=3D"pl-k">def</span> <span class=3D"pl-en">disconnect_succee=
ded</span>(<span class=3D"pl-s1">self</span>):
        <span class=3D"pl-en">super</span>().<span class=3D"pl-en">disconne=
ct_succeeded</span>()
        <span class=3D"pl-en">print</span>(<span class=3D"pl-s">"[%s] Disco=
nnected"</span> <span class=3D"pl-c1">%</span> (<span class=3D"pl-s1">self<=
/span>.<span class=3D"pl-s1">mac_address</span>))

    <span class=3D"pl-k">def</span> <span class=3D"pl-en">services_resolved=
</span>(<span class=3D"pl-s1">self</span>):
        <span class=3D"pl-en">super</span>().<span class=3D"pl-en">services=
_resolved</span>()

        <span class=3D"pl-en">print</span>(<span class=3D"pl-s">"[%s] Resol=
ved services"</span> <span class=3D"pl-c1">%</span> (<span class=3D"pl-s1">=
self</span>.<span class=3D"pl-s1">mac_address</span>))
        <span class=3D"pl-k">for</span> <span class=3D"pl-s1">service</span=
> <span class=3D"pl-c1">in</span> <span class=3D"pl-s1">self</span>.<span c=
lass=3D"pl-s1">services</span>:
            <span class=3D"pl-en">print</span>(<span class=3D"pl-s">"[%s]  =
Service [%s]"</span> <span class=3D"pl-c1">%</span> (<span class=3D"pl-s1">=
self</span>.<span class=3D"pl-s1">mac_address</span>, <span class=3D"pl-s1"=
>service</span>.<span class=3D"pl-s1">uuid</span>))
            <span class=3D"pl-k">for</span> <span class=3D"pl-s1">character=
istic</span> <span class=3D"pl-c1">in</span> <span class=3D"pl-s1">service<=
/span>.<span class=3D"pl-s1">characteristics</span>:
                <span class=3D"pl-en">print</span>(<span class=3D"pl-s">"[%=
s]    Characteristic [%s]"</span> <span class=3D"pl-c1">%</span> (<span cla=
ss=3D"pl-s1">self</span>.<span class=3D"pl-s1">mac_address</span>, <span cl=
